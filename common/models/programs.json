{
  "name": "Programs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "about": {
      "type": "string",
      "required": true
    },
    "application_id": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "application": {
      "type": "hasOne",
      "model": "Applications",
      "foreignKey": "application_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
