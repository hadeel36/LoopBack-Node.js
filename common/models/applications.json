{
  "name": "Applications",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "fields": {
      "type": [
        "any"
      ],
      "required": true,
      "properties": {
        "name": {
          "type": "string",
          "required": true
        },
        "type": {
          "type": "any",
          "required": true
        }
      }
    },
    "program_id": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "program": {
      "type": "belongsTo",
      "model": "Programs",
      "foreignKey": "program_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
